#include <iostream>
#include<cstdlib>
#include<vector>
using namespace std;

void solve()
{
    int n;
    cin>>n;
    vector<int>v(n);
    for(int i=0; i<n;i++)
    {
        cin>>v[i];
    }
    int ans = 0;
    for(int i=0;i<n;i++)
    {
        ans = ans^v[i];
    }
    cout<<ans<<"\n";
}

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    solve();
	}
	return 0;
}
